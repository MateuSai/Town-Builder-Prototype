[application]

config/name="Town Builder"
run/main_scene="res://Game.tscn"
config/icon="res://icon.png"

[autoload]

Utils="*res://Autoloads/Utils.gd"
Resources="*res://Autoloads/Resources.gd"
Data="*res://data.cdb"

[display]

window/size/width=256
window/size/height=144
window/size/test_width=896
window/size/test_height=504
window/stretch/mode="2d"
window/stretch/aspect="keep"

[editor_plugins]

enabled=PoolStringArray( "res://addons/godot_pixelorama_importer/plugin.cfg", "res://addons/thejustinwalsh.castledb/plugin.cfg" )

[input]

ui_click={
